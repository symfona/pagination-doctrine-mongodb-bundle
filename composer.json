{
  "name": "symfona/pagination-doctrine-mongodb-bundle",
  "type": "symfony-bundle",
  "description": "Pagination Doctrine MongoDB Bundle",
  "keywords": ["php", "symfony", "pagination", "doctrine", "mongodb", "odm", "bundle"],
  "homepage": "https://github.com/symfona/pagination-doctrine-mongodb-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Sofy Korzhova",
      "email": "sofykorzhova@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "doctrine/mongodb-odm-bundle": "^4.3",
    "symfona/pagination-bundle": "^2.0",
    "symfony/framework-bundle": "^5.3"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12",
    "phpunit/phpunit": "^9.5",
    "symfony/yaml": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "Symfona\\Pagination\\Doctrine\\MongoDB\\AdapterBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Symfona\\Pagination\\Doctrine\\MongoDB\\AdapterBundle\\Tests\\": "tests"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
