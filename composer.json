{
  "name": "symfona/demo-bundle",
  "type": "symfony-bundle",
  "description": "Symfony Bundle Template",
  "keywords": ["php", "symfony", "bundle", "demo", "template"],
  "homepage": "https://github.com/symfona/demo-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Sofy Korzhova",
      "email": "sofykorzhova@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "symfony/framework-bundle": "^5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "Symfona\\DemoBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Symfona\\DemoBundle\\Tests\\": "tests"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
